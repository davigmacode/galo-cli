{
  "engine": {
    "name": "GALO",
    "description": "Generative Artworks Lies On-chain",
    "version": 1.0
  },
  "traits": {
    "path": "/traits",
    "config": "traits.json",
    "delimiter": "__",
    "exts": [".png", ".jpg"]
  },
  "generations": [],
  "artworks": {
    "path": "/artworks",
    "ext": ".png",
    "width": 1080,
    "height": 1080
  },
  "metadata": {
    "path": "/metadata",
    "config": "metadata.json",
    "shuffle": 3,
    "template": {
      "name": "{{faker.hacker.adjective()}} {{faker.animal.type()}} #{{edition}}",
      "symbol": "GALO",
      "description": "Generative Artworks Lies On-chain",
      "seller_fee_basis_points": 500,
      "external_url": "https://example.com",
      "image": "{{artwork.ipfs}}",
      "attributes": {
        "{{#each attributes}}": {
          "trait_type": "{{traitType.label}}",
          "value": "{{traitItem.label}}"
        }
      },
      "collection": {
        "name": "Collection Name",
        "family": "Collection family Name"
      },
      "properties": {
        "files": [
          {
            "uri": "{{artwork.ipfs}}",
            "type": "image/png"
          }
        ],
        "category": "image",
        "creators": [
          {
            "address": "solana address for a creator",
            "share": 100
          }
        ]
      }
    }
  },
  "collage": {
    "type": "image/png",
    "name": "preview.png",
    "editions": 100,
    "thumbWidth": 50,
    "thumbPerRow": 5
  },
  "storage": {
    "ipfs": {
      "label": "IPFS",
      "cache": "/storage/ipfs.json",
      "token": ""
    },
    "arweave": {
      "label": "Arweave",
      "cache": "/storage/arweave.json",
      "keyfile": "/storage/arweave-key.json"
    },
    "gdrive": {
      "label": "Google Drive",
      "cache": "/storage/gdrive.json",
      "credentials": "/storage/gdrive-credentials.json",
      "token": "/storage/gdrive-token.json"
    }
  },
  "rarity": {
    "C": {
      "label": "Common",
      "weight": 0.9,
      "default": true
    },
    "U": {
      "label": "Uncommon",
      "weight": 0.7
    },
    "R": {
      "label": "Rare",
      "weight": 0.5
    },
    "E": {
      "label": "Epic",
      "weight": 0.3
    },
    "L": {
      "label": "Legend",
      "weight": 0.1
    }
  }
}